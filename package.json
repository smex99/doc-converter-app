{
	"name": "doc-converter",
	"version": "1.0.0",
	"description": "Backend for converting image to pdf",
	"main": "index.js",
	"scripts": {
		"server": "nodemon app/index.js",
		"client": "npm start --prefix client",
		"start": "concurrently \"npm run server \" \"npm run client\"",
		"watch": "nodemon --watch server/index.js",
		"dev": "nodemon --exec npm start"
	},
	"author": "Amine AMEZIANE",
	"license": "MIT",
	"dependencies": {
		"@hapi/joi": "^15.1.0",
		"bcryptjs": "^2.4.3",
		"body-parser": "^1.19.0",
		"concurrently": "^5.3.0",
		"cors": "^2.8.5",
		"cross-env": "^7.0.2",
		"express": "^4.17.1",
		"express-fileupload": "^1.2.0",
		"express-promise-router": "^4.0.1",
		"file-saver": "^2.0.2",
		"jsonwebtoken": "^8.5.1",
		"mongoose": "^5.10.0",
		"morgan": "^1.10.0",
		"multer": "^1.4.2",
		"nodemailer": "^6.4.11",
		"passport": "^0.4.1",
		"passport-jwt": "^4.0.0",
		"passport-local": "^1.0.0",
		"pdfkit": "^0.11.0",
		"tesseract.js": "^2.1.1"
	},
	"devDependencies": {
		"nodemon": "^2.0.4"
	}
}
